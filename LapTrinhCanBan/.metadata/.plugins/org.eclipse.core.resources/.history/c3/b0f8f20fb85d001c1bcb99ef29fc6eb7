package Ex_4_4_Shape;

public class Rectangle extends AShape{
	private int hight;
	private int width;
	/**
	 * CartPT c1 = new CartPT(1,2);
	 * IShape s4 = new Rectangle(c1,10,20)
	 * @param loc
	 * @param x
	 * @param y
	 */
	public Rectangle(CartPT loc,int x, int y) {
		this.loc = loc;
		this.x = x;
		this.y = y;
	}
	
}
