package Ex_4_1_BankAccount;

public class Date {
	private int day;
	private int morth;
	private int year;
	/**
		new Date(12, 23, 2021);
	 * @param day
	 * @param morth
	 * @param year
	 */
	public Date(int day, int morth, int year) {
		this.day = day;
		this.morth = morth;
		this.year = year;
	}
	public int getDay() {
		return day;
	}
	public void setDay(int day) {
		this.day = day;
	}
	public int getMorth() {
		return morth;
	}
	public void setMorth(int morth) {
		this.morth = morth;
	}
	public int getYear() {
		return year;
	}
	public void setYear(int year) {
		this.year = year;
	}
	
}
